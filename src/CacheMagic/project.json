{
  "version": "1.1.0-*",
  "description": "A c# library for in-memory caching, using jitter and retries for stable systems at scale",

  "packOptions": {
    "projectUrl": "https://github.com/JorritSalverda/CacheMagic",
    "licenseUrl": "https://raw.githubusercontent.com/JorritSalverda/CacheMagic/master/LICENSE",
    "repository": {
      "type": "git",
      "url": "git://github.com/JorritSalverda/CacheMagic.git"
    }
  },

  "authors": [ "Jorrit Salverda" ],
  "tooling": {
    "defaultNamespace": "CacheMagic"
  },
  
  "buildOptions": {
     "warningsAsErrors": true
  },

  "dependencies": {
    "RetryMagic": "1.1.1",
    "Microsoft.Extensions.Caching.Memory": "1.0.0"
  },  

  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    },
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": "1.0.0"
      }
    },
    "net46": {
    }
  }
}